<div class="listings-box">
  <%= link_to (image_tag listing.image(:medium)), listing %>

  <p class="listing-title">
    <%= listing.title %>
  </p>

  <p class="listing-postedby">
    <strong>
      Posted by <%= link_to listing.user.name, listing.user  %>
    </strong>
  </p>

</div>
